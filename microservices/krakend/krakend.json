{
  "version": 2,
  "endpoints": [
    {
      "endpoint": "/Api/LearningOrchestra/Dataset/{filename}",
      "backend": [
        {
          "url_pattern": "/files/{filename}",
          "host": [
            "http://databaseapi:5000"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Transform/Projection/{filename}",
      "backend": [
        {
          "url_pattern": "/projections/{filename}",
          "host": [
            "http://projection:5001"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Transform/DataType/{filename}",
      "backend": [
        {
          "url_pattern": "/fieldTypes/{filename}",
          "host": [
            "http://datatypehandler:5003"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Explore/Tsne/{filename}",
      "backend": [
        {
          "url_pattern": "/images/{filename}",
          "host": [
            "http://tsne:5005"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Explore/Pca/{filename}",
      "backend": [
        {
          "url_pattern": "/images/{filename}",
          "host": [
            "http://pca:5006"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Explore/Histogram/{filename}",
      "backend": [
        {
          "url_pattern": "/histograms/{filename}",
          "host": [
            "http://histogram:5004"
          ],
          "extra_config": {}
        }
      ]
    },
    {
      "endpoint": "/Api/LearningOrchestra/Builder",
      "backend": [
        {
          "url_pattern": "/models",
          "host": [
            "http://modelbuilder:5002"
          ],
          "extra_config": {}
        }
      ]
    }
  ],
  "extra_config": {
    "github_com/devopsfaith/krakend-metrics": {
      "collection_time": "60s",
      "proxy_disabled": false,
      "router_disabled": false,
      "backend_disabled": false,
      "endpoint_disabled": false,
      "listen_address": ":8090"
    },
    "github_com/devopsfaith/krakend-gologging": {
      "level": "ERROR",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "default"
    }
  },
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "gatewayapi"
}